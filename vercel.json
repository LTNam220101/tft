{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "functions": {
        "server/server.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/client/assets/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/server/server.js"
        }
    ]
}